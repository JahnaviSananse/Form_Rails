<% provide(:title, 'Sign in') %>
<h1>Sign in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @form, local: true) do |f| %>
     
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "SIGN IN", class: "btn btn-primary" %>


    <% end %>
  </div>
</div>
